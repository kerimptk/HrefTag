@using HrefTag.WebUI.ViewModels;
@model HeadViewModel;


<meta name="author" content="Kerim Potuk">
<meta name="description" content="@Html.Raw(@Model.seoAyarlariDto.Description)">
<meta name="keywords" content="@Html.Raw(@Model.seoAyarlariDto.Keywords)">
<title>@ViewData["Title"] | @Html.Raw(@Model.seoAyarlariDto.Title)</title>


@Html.Raw(Model.seoAyarlariDto.Analytics)
@Html.Raw(Model.seoAyarlariDto.Adsense)

@foreach (var item in Model.genelAyarlarDtos)
{
    <link rel="shortcut icon" href="/uploads/@item.Favicon" type="image/x-icon" />
}