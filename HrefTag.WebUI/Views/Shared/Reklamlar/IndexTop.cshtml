@model List<ReklamAlanlariDto>
<div class="clearfix visible-md visible-lg"></div>

<div class="row">
    <div class="col-md-12 m-4 p-4">
        @foreach (var item in Model)
        {
            if (item.AlanAdi.Equals("IndexTop"))
            {
                @Html.Raw(@item.Kodu)
            }
        }
    </div>
</div>
<div class="clearfix visible-md visible-lg"></div>